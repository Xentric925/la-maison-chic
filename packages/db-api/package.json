{
  "name": "db-api",
  "version": "1.0.0",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/index.js",
    "dev": "nodemon --legacy-watch src/index.ts",
    "lint": "eslint . --fix",
    "format": "prettier --write .",
    "lint:check": "eslint .",
    "format:check": "prettier --check .",
    "db:seed": "docker compose exec db-api npx tsx src/seed/seed.ts",
    "db:reset": "docker compose exec db-api npx prisma migrate reset",
    "db:migrate": "docker compose exec db-api npx prisma migrate dev",
    "test:db:seed": "docker compose exec test-db-api npx tsx src/seed/seed.ts",
    "test:db:reset": "docker compose exec test-db-api npx prisma migrate reset",
    "test:db:migrate": "docker compose exec test-db-api npx prisma migrate dev",
    "test": "jest"
  },
  "dependencies": {
    "@prisma/client": "^5.19.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/ioredis": "^5.0.0",
    "@types/jest": "^29.5.13",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.14.10",
    "@types/supertest": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^10.0.0",
    "@types/yamljs": "^0.2.34",
    "concurrently": "^8.2.2",
    "dotenv": "^16.4.5",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.7",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.2",
    "prisma": "^5.19.1",
    "supertest": "^7.0.0",
    "swagger-ui-express": "^5.0.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3",
    "yamljs": "^0.3.0"
  }
}
